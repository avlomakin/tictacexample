        xmlns:local="clr-namespace:UltraTT"
        mc:Ignorable="d"
        Title="UltraTT" Height="400" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Frame Name="FF"  Grid.Row="0" Height="71" Source="Game/TopHubPageView.xaml"></Frame>
        <Grid Grid.Row="1">
