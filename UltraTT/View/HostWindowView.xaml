                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Frame Name="WorkingFrame" Grid.Column="1" Source="Game/HotSeatPageView.xaml"/>
        </Grid>
        
    </Grid>
</Window>
